{
    "id": "blog",
    "version": 2,
    "entityTypes": {
        "entry": {
            "properties": {
                "title": {
                    "type": "STRING"
                },
                "published": {
                    "type": "BOOLEAN"
                },
                "publishDate": {
                    "type": "LOCALDATE"
                },
                "created": {
                    "type": "TIMESTAMP"
                },
                "category": {
                    "type": {
                        "ENUM": ["POLITICS", "SPORTS"]
                    }
                },
                "readcount": {
                    "type": "INTEGER"
                },
                "tags": {
                    "type": "STRING",
                    "structure": "LIST"
                },
                "author": {
                    "type": {
                        "EMBEDDED": {
                            "properties": {
                                "name": {
                                    "type": "STRING"
                                },
                                "subtext": {
                                    "type": "STRING"
                                }
                            }
                        }
                    }
                },
                "ratings": {
                    "type": "INTEGER",
                    "structure": "MAP"
                },
                "sponsor": {
                    "type": "STRING"
                }
            },
            "migrations": {
                "addSponsorField": {
                    "schemaVersion": 2,
                    "type": {
                        "ADD_PROPERTIES": {
                            "sponsor": "no sponsor"
                        }
                    }
                }
            }
        }
    }
}
